interface Category{
	id: String!
	name: String!
}

type Query {
	findAll(
	page: Int,
	linesPerPage: Int,
	orderBy: String,
	direction: String,	
	): [Category!]!
	findById(id: String!): Category
}

type Mutation {
	delete(id: String!): String
	update(id: String!, name: String!): Category
	insert(id: String!, name: String!): Category
}